public class Laptop {
	private int cpu;
	private int resolution;
	private int usbNumbers;

	public int getCpu() {
		return cpu;
	}

	public abstract void setCpu(int cpu) {
		this.cpu = cpu;
	}

	public int getResolution() {
		return resolution;
	}

	public abstract void setResolution(int resolution) {
		this.resolution = resolution;
	}

	public int getUsbNumbers() {
		return usbNumbers;
	}

	public abstract void setUsbNumbers(int usbNumbers) {
		this.usbNumbers = usbNumbers;
	}

}
